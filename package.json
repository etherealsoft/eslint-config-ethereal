{
  "name": "@ethereal-soft/eslint-config-ethereal",
  "version": "2.0.0",
  "description": "Recommended eslint configurations.",
  "author": "Stephen M. Senesac <stephen.senesac@ethereal-soft.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/etherealsoft/eslint-config-ethereal.git"
  },
  "keywords": [
    "eslint",
    "config",
    "sharable"
  ],
  "main": "es5.js",
  "files": [
    "lib/**/*.js",
    "rules/**/*.js",
    "es5.js",
    "es2015.js",
    "es2016.js",
    "es2017.js",
    "babel-es2015.js",
    "babel-es2016.js",
    "babel-es2017.js",
    "babel-stage-2.js",
    "node.js",
    "react.js",
    "flowtype.js",
    "jsdoc.js"
  ],
  "engines": {
    "node": ">=6.x"
  },
  "dependencies": {
    "babel-eslint": "8.2.2",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-react": "7.7.0"
  },
  "devDependencies": {
    "lodash": "~4.17.4",
    "mocha": "~4.1.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha ./tests/**/*.test.js",
    "check": "npm run lint && npm test",
    "prepublishOnly": "npm run check"
  }
}
