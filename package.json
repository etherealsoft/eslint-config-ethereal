{
  "name": "@ethereal-soft/eslint-config-ethereal",
  "version": "2.1.0",
  "description": "Recommended eslint configurations.",
  "author": "Stephen M. Senesac <stephen.senesac@ethereal-soft.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/etherealsoft/eslint-config-ethereal.git"
  },
  "keywords": [
    "eslint",
    "config",
    "sharable"
  ],
  "main": "es5.js",
  "files": [
    "lib/**/*.js",
    "rules/**/*.js",
    "es5.js",
    "es2015.js",
    "es2016.js",
    "es2017.js",
    "babel-es2015.js",
    "babel-es2016.js",
    "babel-es2017.js",
    "babel-stage-2.js",
    "nodejs.js",
    "react.js",
    "flowtype.js",
    "jsdoc.js"
  ],
  "engines": {
    "node": ">=8.10.x"
  },
  "dependencies": {
    "babel-eslint": "9.0.0",
    "eslint": "5.4.0",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1"
  },
  "devDependencies": {
    "lodash": "~4.17.10",
    "mocha": "~5.2.0"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha ./tests/**/*.test.js",
    "check": "npm run lint && npm run test"
  }
}
